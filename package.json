{
  "name": "ashley-calendar-ai-test-harness",
  "version": "1.0.0",
  "description": "Test harness for OpenAI Assistant calendar intent extraction using BAML",
  "main": "dist/run-test.js",
  "scripts": {
    "build": "tsc",
    "test": "node --loader tsx --test './test-intent.ts'",
    "test:smoke": "grep -l '@smoke' *.ts | head -5 | xargs -I {} ts-node {}",
    "test:full": "npm run test",
    "test-intent": "ts-node test-intent.ts",
    "test-ashley": "ts-node test-ashley.ts",
    "test-context": "ts-node test-context.ts",
    "test-sid-multi-participant": "ts-node test-sid-multi-participant.ts",
    "test-speech-formatter": "ts-node test-speech-formatter.ts",
    "test-calendar-data-simple": "ts-node test-calendar-data-simple.ts",
    "setup-oauth": "ts-node setup-oauth.ts",
    "orchestrator": "ts-node src/orchestrator/main.ts",
    "orchestrator:once": "ts-node src/orchestrator/main.ts --once",
    "orchestrator:dev": "ts-node src/orchestrator/main.ts --interval=1",
    "test-oauth-consent": "ts-node test-oauth-consent.ts",
    "dev": "baml dev",
    "generate": "npx baml-cli generate",
    "lint": "eslint *.ts",
    "lint:fix": "eslint *.ts --fix"
  },
  "keywords": [
    "openai",
    "assistant",
    "calendar",
    "baml",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@baml/client": "^0.65.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@types/node": "^20.14.10",
    "dotenv": "^16.4.5",
    "googleapis": "^140.0.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.8.0",
    "typescript-eslint": "^8.0.0"
  }
}