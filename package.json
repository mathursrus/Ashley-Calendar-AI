{
  "name": "ashley-calendar-ai",
  "version": "1.0.0",
  "description": "AI-powered calendar assistant",
  "main": "index.js",
  "scripts": {
    "test": "bun test",
    "test:smoke": "grep -l '@smoke' test-*.ts | xargs bun test --test-name-pattern='@smoke'",
    "dev": "bun run --watch index.ts",
    "build": "bun build index.ts --outdir ./dist",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts,.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["ai", "calendar", "assistant", "scheduling"],
  "author": "Ashley Calendar AI Team",
  "license": "MIT",
  "dependencies": {
    "@boundaryml/baml": "^0.65.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}