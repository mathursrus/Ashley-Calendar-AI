{
  "name": "ashley-calendar-ai-test-harness",
  "version": "1.0.0",
  "description": "Test harness for OpenAI Assistant calendar intent extraction using BAML",
  "main": "dist/run-test.js",
  "scripts": {
    "build": "tsc",
    "test-intent": "ts-node test-intent.ts",
    "test-ashley": "ts-node test-ashley.ts",
    "test-context": "ts-node test-context.ts",
    "test-owner-intent": "ts-node test-owner-intent.ts",
    "test-speech-formatter": "ts-node test-speech-formatter.ts",
    "dev": "baml dev",
    "generate": "npx baml-cli generate",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "openai",
    "assistant",
    "calendar",
    "baml",
    "testing",
    "ai"
  ],
  "author": "Ashley Calendar AI",
  "license": "MIT",
  "devDependencies": {
    "@boundaryml/baml": "^0.204.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "dotenv": "^16.6.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
