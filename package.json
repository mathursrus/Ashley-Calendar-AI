{
  "name": "ashley-calendar-ai",
  "version": "1.0.0",
  "description": "An intelligent calendar assistant built with n8n that automatically manages email-based meeting requests and calendar scheduling",
  "main": "index.js",
  "scripts": {
    "test": "tsx test-*.ts",
    "test:smoke": "grep -l '@smoke' test-*.ts | xargs tsx",
    "test:ashley": "tsx test-ashley.ts",
    "test:intent": "tsx test-intent.ts",
    "test:context": "tsx test-context.ts",
    "test:speech": "tsx test-speech-formatter.ts",
    "test:multi": "tsx test-sid-multi-participant.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "keywords": [
    "calendar",
    "assistant",
    "ai",
    "scheduling",
    "email",
    "automation"
  ],
  "author": "Sid Mathur",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0"
  }
}