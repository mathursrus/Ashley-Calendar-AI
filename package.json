{
  "name": "ashley-calendar-ai",
  "version": "1.0.0",
  "description": "An intelligent calendar assistant built with n8n that automatically manages email-based meeting requests and calendar scheduling",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "node --loader tsx --test './test-intent.ts'",
    "test:smoke": "grep -l '@smoke' *.ts | head -5 | xargs -I {} ts-node {}",
    "test-intent": "ts-node test-intent.ts",
    "test-ashley": "ts-node test-ashley.ts",
    "test-context": "ts-node test-context.ts",
    "test-calendar-data-simple": "ts-node test-calendar-data-simple.ts",
    "test-speech-formatter": "ts-node test-speech-formatter.ts",
    "test-runner": "ts-node test-runner.ts",
    "test-multi-participant": "ts-node test-multi-participant-runner.ts",
    "generate": "baml-cli generate",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "cp -r src/assets dist/ || true",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "prepare": "husky install || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "dotenv": "^16.4.5",
    "googleapis": "^144.0.0",
    "openai": "^4.67.1"
  },
  "devDependencies": {
    "@boundaryml/baml": "^0.204.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  }
}